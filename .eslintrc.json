{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-undef": "off",
        "avoidEscape": 0,
        "no-use-before-define":"off",
        "linebreak-style":"off",
        "no-plusplus":"off",
        "prefer-const":"warn",
        "no-unused-vars":"off",
        "no-unused-expressions":"off",
        "quotes":"warn",
        "no-restricted-syntax":"off",
        "guard-for-in":"off",
        "comma-dangle":"off",
        "operator-linebreak":"warn",
        "no-throw-literal":"warn"
    }
}
